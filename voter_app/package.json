    "animate.css": "^4.1.1",
    "axios": "^1.13.5",
    "base32": "^0.0.7",
    "bootstrap": "^5.3.8",
    "country-flag-emoji-polyfill": "^0.1.8",
    "js-sha256": "^0.11.1",
    "qrcode.vue": "^3.8.0",
